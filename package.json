{
  "name": "@sefirosweb/react-crud",
  "version": "1.2.0",
  "description": "Basic components for CRUD with react",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "homepage": "https://sefirosweb.github.io/react-crud",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "eslint . --fix",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "deploy-storybook": "cp storybook-static/index.html storybook-static/404.html && gh-pages -d storybook-static",
    "build-lib": "rollup -c",
    "start": "npm run storybook",
    "build": "npm run build-lib",
    "docker": "docker run --rm -it --name node-docker-storybook -v ~/.gitconfig:/etc/gitconfig -v ~/.gnupg:/home/node/.gnupg -v $PWD:/home/app -w /home/app -e \"NODE_OPTIONS=--openssl-legacy-provider\" -p 6006:6006 -u node node:17.2.0-bullseye /bin/bash",
    "prepare": "husky install"
  },
  "author": "@Sefirosweb",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.16.5",
    "@babel/preset-react": "^7.16.5",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-node-resolve": "^13.1.1",
    "@storybook/addon-actions": "^6.4.9",
    "@storybook/addon-essentials": "^6.4.9",
    "@storybook/addon-links": "^6.4.9",
    "@storybook/preset-scss": "^1.0.3",
    "@storybook/react": "^6.4.9",
    "axios": "^0.24.0",
    "axios-mock-adapter": "^1.20.0",
    "babel-loader": "^8.2.3",
    "bootstrap": "^5.1.3",
    "eslint": "^8.5.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.28.0",
    "gh-pages": "^3.2.3",
    "husky": "^7.0.4",
    "lint-staged": "^12.1.4",
    "lodash": "^4.17.21",
    "postcss": "^8.4.5",
    "prettier": "^2.5.1",
    "prop-types": "^15.8.0",
    "react": "^17.0.2",
    "react-bootstrap": "^2.0.4",
    "react-dom": "^17.0.2",
    "react-icons": "^4.3.1",
    "react-table": "^7.7.0",
    "rollup": "^2.62.0",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-terser": "^7.0.2",
    "sass": "^1.45.1",
    "storybook-addon-sass-postcss": "^0.1.0",
    "storybook-addon-state": "^1.0.3",
    "toastr": "^2.1.4"
  },
  "peerDependencies": {
    "axios": "^0.24.0",
    "bootstrap": "^5.1.3",
    "lodash": "^4.17.21",
    "prop-types": "^15.8.0",
    "react": "^17.0.2",
    "react-bootstrap": "^2.0.4",
    "react-dom": "^17.0.2",
    "react-icons": "^4.3.1",
    "react-table": "^7.7.0",
    "toastr": "^2.1.4"
  },
  "lint-staged": {
    "*.js": "eslint --cache --fix"
  }
}