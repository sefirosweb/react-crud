{
  "name": "@sefirosweb/react-crud",
  "version": "1.3.0",
  "description": "Basic components for CRUD with react",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "homepage": "https://sefirosweb.github.io/react-crud",
  "scripts": {
    "test": "react-scripts test",
    "lint": "eslint . --fix",
    "storybook": "start-storybook -p 6006 -s public",
    "build-lib": "rollup -c",
    "start": "npm run storybook",
    "react": "react-scripts start",
    "build-react": "react-scripts build",
    "eject": "react-scripts eject",
    "build-storybook": "build-storybook -s public",
    "build": "npm run build-lib",
    "docker": "docker run --rm -it --name node-docker-storybook -v ~/.gitconfig:/etc/gitconfig -v ~/.gnupg:/home/node/.gnupg -v $PWD:/home/app -w /home/app -e \"NODE_OPTIONS=--openssl-legacy-provider\" -p 6006:6006 -u node node:17.2.0-bullseye /bin/bash",
    "prepare": "husky install",
    "update": "npx npm-check-updates"
  },
  "author": "@Sefirosweb",
  "license": "MIT",
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@rollup/plugin-babel": "^5.3.1",
    "@rollup/plugin-node-resolve": "^13.2.1",
    "@storybook/addon-actions": "^6.4.22",
    "@storybook/addon-essentials": "^6.4.22",
    "@storybook/addon-interactions": "^6.4.22",
    "@storybook/addon-links": "^6.4.22",
    "@storybook/builder-webpack5": "^6.4.22",
    "@storybook/manager-webpack5": "^6.4.22",
    "@storybook/node-logger": "^6.4.22",
    "@storybook/preset-create-react-app": "^4.1.0",
    "@storybook/preset-scss": "^1.0.3",
    "@storybook/react": "^6.4.22",
    "@storybook/testing-library": "^0.0.11",
    "axios": "^0.27.2",
    "axios-mock-adapter": "^1.20.0",
    "bootstrap": "^5.1.3",
    "css-loader": "^6.7.1",
    "eslint": "^8.14.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.29.4",
    "gh-pages": "^3.2.3",
    "husky": "^7.0.4",
    "lodash": "^4.17.21",
    "prop-types": "^15.8.1",
    "react": "^17.0.2",
    "react-bootstrap": "^2.3.0",
    "react-dom": "^17.0.2",
    "react-icons": "^4.3.1",
    "react-scripts": "^5.0.1",
    "react-table": "^7.7.0",
    "rollup": "^2.70.2",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-terser": "^7.0.2",
    "sass": "^1.51.0",
    "sass-loader": "^12.6.0",
    "storybook-addon-state": "^1.0.3",
    "style-loader": "^3.3.1",
    "toastr": "^2.1.4",
    "web-vitals": "^2.1.4",
    "webpack": "^5.72.0"
  },
  "peerDependencies": {
    "axios": "^0.27.2",
    "bootstrap": "^5.1.3",
    "lodash": "^4.17.21",
    "prop-types": "^15.8.0",
    "react": "^17.0.2",
    "react-bootstrap": "^2.3.0",
    "react-dom": "^17.0.2",
    "react-icons": "^4.3.1",
    "react-table": "^7.7.0",
    "toastr": "^2.1.4"
  },
  "lint-staged": {
    "*.js": "eslint --cache --fix"
  },
  "dependencies": {
    "react": "^17.0.2"
  }
}