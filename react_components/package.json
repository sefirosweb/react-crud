  },
  "bugs": {
    "url": "https://github.com/sefirosweb/react-crud/issues"
  },
  "dependencies": {
    "@tanstack/react-query": "^4.20.4",
    "@tanstack/react-table": "8.5.13",
    "axios": "0.27.2",
    "luxon": "3.1.0",
    "react": "^18.2.0",
    "react-bootstrap": "2.5.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.4.0",
    "toastr": "^2.1.4"
  },
  "devDependencies": {
    "@faker-js/faker": "7.5.0",
    "@types/luxon": "^3.1.0",
    "@types/node": "^18.11.9",
    "@types/react": "18.0.20",
    "@types/react-dom": "^18.0.9",
