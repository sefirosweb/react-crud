{
  "name": "@sefirosweb/react-crud",
  "version": "2.9.16",
  "author": "@Sefirosweb",
  "license": "MIT",
  "description": "Basic components for CRUD with react",
  "type": "module",
  "main": "dist/index.umd.js",
  "module": "dist/index.es.js",
  "types": "dist/index.d.ts",
  "private": false,
  "exports": {
    ".": {
      "import": "./dist/index.es.js",
