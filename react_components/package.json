  "license": "MIT",
  "description": "Basic components for CRUD with react",
  "type": "module",
  "main": "./dist/react-crud.umd.cjs",
  "module": "./dist/react-crud.js",
  "types": "./dist/index.d.ts",
  "private": false,
  "exports": {
    ".": {
      "import": "./dist/react-crud.js",
      "require": "./dist/react-crud.umd.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "react",
    "crud",
    "table",
    "modal"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "dev": "vite",
    "build": "cp ../README.md ./ && tsc && vite build",
    "pack": "npm run build && rm sefirosweb-react-crud-* && npm pack && mv sefirosweb-react-crud-* sefirosweb-react-crud-$(date +%Y%m%d-%H%M%S).tgz",
    "preview": "vite preview"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/sefirosweb/react-crud.git"
  },
  "bugs": {
    "url": "https://github.com/sefirosweb/react-crud/issues"
  },
